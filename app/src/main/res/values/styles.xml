<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="roundedImageVRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="animation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="roundedImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>
    <style name="roundedImage1">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">@dimen/_12sdp</item>
        <item name="cornerSizeBottomLeft">@dimen/_12sdp</item>

    </style>

    <style name="SearchViewTheme" parent="Widget.AppCompat.SearchView">
        <item name="android:textColorHint">#ACACAC</item>
        <item name="android:editTextColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="queryHint">search</item>
        <item name="iconifiedByDefault">false</item>
    </style>

    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
    <item name="android:textSize">18sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:fontFamily">@font/montserrat_semi_bold</item>
    </style>

</resources>